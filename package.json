{
  "name": "stack-pack-gpapi-demo",
  "description": "Get started with the GoodPractice API (GPAPI) using this introductory demo application",
  "version": "0.0.1",
  "author": "GoodPractice",
  "license": "UNLICENSED",
  "homepage": "https://github.com/gp-technical/stack-pack-gpapi-demo/readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gp-technical/stack-pack-gpapi-demo"
  },
  "main": "src/index.js",
  "scripts": {
    "lint": "sp-lint",
    "precommit": "sp-lint-staged",
    "start:debug": "nodemon --exec yarn start --inspect --source-maps inline",
    "start": "env-cmd .env babel-node src/index.js --silent",
    "production": "babel --presets env,stage-2 src --out-dir /build"
  },
  "standard-engine": "@gp-technical/stack-pack-standard",
  "nodemonConfig": {
    "watch": [
      "src",
      ".env",
      "node_modules/@gp-technical"
    ],
    "ignoreRoot": [
      "yarn.lock",
      "package.json"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.13.4",
    "le_node": "^1.7.0",
    "sparkpost": "^2.1.2",
    "stack-pack-gpapi": "2.1.1",
    "unirest": "^0.5.1"
  },
  "devDependencies": {
    "@gp-technical/stack-pack-standard": "^2.3.5",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.16.0",
    "env-cmd": "^8.0.0",
    "nodemon": "^1.17.5"
  }
}
